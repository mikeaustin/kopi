dict = { "One": 1, "Two": 2 } | map (key, value) => {
  value * value
}

print $ dict | get "One"

dict = dict | set ("One", 3)

print $ dict | get "One"

print $ match (dict | get "Three") (
  ()    => 100
  value => value
)

