radians (degrees) = degrees * (Number.PI / 180.0)

print $ radians 90

randomInt (range) = 'floor (random () * (range.to - range.from + 1)) + range.from

print $ randomInt 0..5

randomReal (range) = (random () * (range.to - range.from)) + range.from

print $ randomReal 0..5

