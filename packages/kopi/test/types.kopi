Point = type (x: Int, y: Int)

point = Point (x: 1, y: 1)

print $ point.x, point.y

extend Point (
  length: (this) => () => (this.x * this.x + this.y * this.y) | sqrt
)

print $ point | length

{
  extend Point (
    length: (this) => () => "qux"
  )

  print $ point | length
}

print $ point | length
